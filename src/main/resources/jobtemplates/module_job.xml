<?xml version='1.1' encoding='UTF-8'?>
<project>
    <actions/>
    <description>Android组件构建：修改版本号，打包aar上传maven仓库，提交代码</description>
    <displayName>Android组件Girls</displayName>
    <keepDependencies>false</keepDependencies>
    <properties>
        <com.dabsquared.gitlabjenkins.connection.GitLabConnectionProperty plugin="gitlab-plugin@1.5.11">
            <gitLabConnection></gitLabConnection>
        </com.dabsquared.gitlabjenkins.connection.GitLabConnectionProperty>
        <hudson.model.ParametersDefinitionProperty>
            <parameterDefinitions>
                <hudson.model.StringParameterDefinition>
                    <name>MODULE_ID</name>
                    <description>组件ID：用于保存构建记录，通知后端</description>
                    <defaultValue></defaultValue>
                    <trim>false</trim>
                </hudson.model.StringParameterDefinition>
                <hudson.model.StringParameterDefinition>
                    <name>CATALOG</name>
                    <description>工程目录</description>
                    <defaultValue></defaultValue>
                    <trim>false</trim>
                </hudson.model.StringParameterDefinition>
                <hudson.model.StringParameterDefinition>
                    <name>MODULE_NAME</name>
                    <description>模块名称</description>
                    <defaultValue></defaultValue>
                    <trim>false</trim>
                </hudson.model.StringParameterDefinition>
                <hudson.model.StringParameterDefinition>
                    <name>AAR_VERSION</name>
                    <description>aar发布版本号</description>
                    <defaultValue></defaultValue>
                    <trim>false</trim>
                </hudson.model.StringParameterDefinition>
                <net.uaznia.lukanus.hudson.plugins.gitparameter.GitParameterDefinition plugin="git-parameter@0.9.10">
                    <name>BRANCH</name>
                    <description></description>
                    <uuid>5a46362a-83b7-42a3-aa56-1ddd1a2b39ff</uuid>
                    <type>PT_BRANCH</type>
                    <branch></branch>
                    <tagFilter>*</tagFilter>
                    <branchFilter>.*</branchFilter>
                    <sortMode>NONE</sortMode>
                    <defaultValue>origin/master</defaultValue>
                    <selectedValue>NONE</selectedValue>
                    <quickFilterEnabled>false</quickFilterEnabled>
                    <listSize>5</listSize>
                </net.uaznia.lukanus.hudson.plugins.gitparameter.GitParameterDefinition>
            </parameterDefinitions>
        </hudson.model.ParametersDefinitionProperty>
    </properties>
    <scm class="hudson.plugins.git.GitSCM" plugin="git@3.9.3">
        <configVersion>2</configVersion>
        <userRemoteConfigs>
            <hudson.plugins.git.UserRemoteConfig>
                <url>https://github.com/moon-lights/CIDemo_Girls.git</url>
                <credentialsId>d92187ae-7076-4800-93b9-9689f9da3b94</credentialsId>
            </hudson.plugins.git.UserRemoteConfig>
        </userRemoteConfigs>
        <branches>
            <hudson.plugins.git.BranchSpec>
                <name>${BRANCH}</name>
            </hudson.plugins.git.BranchSpec>
        </branches>
        <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
        <submoduleCfg class="list"/>
        <extensions/>
    </scm>
    <canRoam>true</canRoam>
    <disabled>false</disabled>
    <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
    <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
    <triggers/>
    <concurrentBuild>false</concurrentBuild>
    <customWorkspace>/var/jenkins_workspace/$CATALOG</customWorkspace>
    <builders>
        <hudson.tasks.Shell>
            <command>
echo 工程目录+$CATALOG
./gradlew --version
python /var/jenkins_workspace/ci_script/module_build.py -m $MODULE_ID -n $MODULE_NAME -v $AAR_VERSION -i $BUILD_NUMBER
            </command>
        </hudson.tasks.Shell>
    </builders>
    <publishers/>
    <buildWrappers/>
</project>